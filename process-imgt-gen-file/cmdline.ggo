package	"process_imgt_gen_file"
purpose	"Process IMGT multiple alignment HLA sequence (_gen.txt) files."
usage	"process_imgt_gen_file < A_gen.txt"
version	"0.1"
description	"Output text files with the HLA type in the first column and the sequence in the second."

option	"output-format"	-	"Output format"	typestr = "FORMAT"	values = "tabulated", "a2m" default = "tabulated"	enum	optional
